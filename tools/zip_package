; ags2demos-menu
IF EXISTS "PACKAGEDIR:package/ags2demos-menu.zip"
  delete >NIL: "PACKAGEDIR:package/ags2demos-menu.zip"
ENDIF
cd "PACKAGEDIR:ags2demos-menu/"
"PACKAGEDIR:tools/zip" -r "PACKAGEDIR:package/ags2demos-menu.zip" *


